{
  "name": "time-tracking-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.1",
    "flow-typed": "^2.0.0",
    "getstorybook": "^1.7.0",
    "global": "^4.3.1",
    "less": "^2.7.2",
    "less-watch-compiler": "^1.4.2",
    "lessc-each": "^1.1.0",
    "material-ui": "^0.17.4",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-tap-event-plugin": "^2.0.1",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.14.6",
    "serve": "^5.1.4"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
